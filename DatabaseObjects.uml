              </ownedParameter>
              <language>java</language>
              <body>String regex = &quot;^(?=.*[0-9])(?=.*[a-z])(?=.*[A-Z])(?=.*[*!$%&amp;@#^])(?=\\S+$).{8,25}$&quot;;&#xD;
		Pattern pattern = Pattern.compile(regex);&#xD;
