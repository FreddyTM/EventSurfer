            <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_AZoQbYGbEeuPkZNKCcfHxw" name="getEventUpdatesFromDB" specification="_AYxUrIGbEeuPkZNKCcfHxw">
